# HTML-formatted email
text/html; html2text -d -e --reference-links --images-to-alt --wrap-list-items %s; nametemplate=%s.html; copiousoutput;
#text/html; elinks -dump %s; nametemplate=%s.html; copiousoutput
#text/html; w3m -I %{charset} -T text/html; copiusoutput;

# Images
image/*; geeqie %s;

# Adobe Portable Document Format
application/pdf; mupdf %s; copiousoutput;
application/PDF; mupdf %s; copiousoutput;

# Handle vcalendar files
text/calendar; mutt-ics; copiousoutput;
application/ics; mutt-ics; copiousoutput;
